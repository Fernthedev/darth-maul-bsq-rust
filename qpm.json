{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.4.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Darth Maul Rust",
    "id": "darth_maul_rust",
    "version": "0.1.0",
    "url": null,
    "additionalData": {
      "overrideSoName": "libdarth_maul_rust.so",
      "cmake": false
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "cargo ndk -t arm64-v8a -o ./build build --release"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "cpp": [
        "pwsh ./scripts/make_compile_db.ps1"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "build/arm64-v8a"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "darth_maul_rust.qmod"
  },
  "dependencies": [
    {
      "id": "bs-cordl",
      "versionRange": "^4008.0.0",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.55",
      "additionalData": {}
    },
    {
      "id": "metacore",
      "versionRange": "^1.6.2",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.7.0",
      "additionalData": {}
    }
  ]
}